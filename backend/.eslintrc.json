{
  "extends": ["google"],
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    "require-jsdoc": "off",
    "valid-jsdoc": "off",
    "max-len": ["error", {"code": 100, "ignoreStrings": true, "ignoreTemplateLiterals": true}],
    "object-curly-spacing": ["error", "never"],
    "indent": ["error", 2],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-unused-vars": ["warn"],
    "camelcase": ["error", {"properties": "never"}]
  }
}